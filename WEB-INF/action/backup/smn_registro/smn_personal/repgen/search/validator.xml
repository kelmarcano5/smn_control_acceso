<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='psl_num_control' type='varchar' required='false' label='${lbl:b_number_of_control}' maxlen='16'/>
	<parameter id='psl_grupo' type='integer' required='false' label='${fld:b_group}' maxlen='10'/>
	<parameter id='psl_grupo_fecha_inicio' type='date' required='false' label='${fld:b_start_date}' maxlen='10'/>
	<parameter id='psl_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='psl_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='psl_empresa' type='integer' required='false' label='${lbl:b_entity}' maxlen='10'/>
	<parameter id='psl_sucursal' type='integer' required='false' label='${lbl:b_subsidiary}' maxlen='10'/>
	<parameter id='psl_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='psl_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='psl_centro_costo' type='integer' required='false' label='${lbl:b_cost_centers}' maxlen='10'/>
	<parameter id='psl_estructura_organiz' type='integer' required='false' label='${lbl:b_organizational_structure}' maxlen='10'/>
	<parameter id='psl_cargo' type='integer' required='false' label='${lbl:b_positions}' maxlen='10'/>
	<parameter id='psl_categoria_cargo' type='integer' required='false' label='${lbl:b_position_category}' maxlen='10'/>
	<parameter id='psl_habilita_acceso' type='varchar' required='false' label='${lbl:b_enables_access_control}' maxlen='1'/>
	<parameter id='psl_id_control_acceso' type='varchar' required='false' label='${lbl:b_id_access_control}' maxlen='16'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='psl_num_control'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='psl_habilita_acceso'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='psl_id_control_acceso'
		rule='contains' />




	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
